<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf-8'>
        <title>Results!</title>
        <link rel="stylesheet" href="~/css/style.css" />
</head>

@model SurveyResults;

<body>
    <h1>Submitted Info</h1>
    <div class="box">
        <div class="formstuff">
            <div class="subject">
                <h3>Name:</h3>
                <h3>Dojo Location:</h3>
                <h3>Favorite Language:</h3>
                <h3>Comment:</h3>
            </div>
            <div class="answer">
                <h3>@Model.Name</h3>
                <h3>@Model.Location</h3>
                <h3>@Model.Language</h3>
                <h3>@Model.Comments</h3>
            </div>
        </div>
        <a class="button" href="http://localhost:5021/">Go Back</a>
    </div>
</body>
</html>